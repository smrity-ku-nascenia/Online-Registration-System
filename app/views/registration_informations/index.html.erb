<br><fieldset>
  <legend>Course Takens</legend>

  <table>
    <thead>
    <tr>
      <th>Student ID</th>
      <th>Semester Name</th>
      <th>Course Name</th>
      <th colspan="3"></th>
    </tr>
    </thead>

    <tbody>

    <% @course_takens.each do |registration| %>
        <tr>
          <td><%= current_user.user_id %></td>
          <td><%= registration.semester.semester_name %></td>
          <td><%= registration.course.course_name %></td>
          <td><%= link_to 'Show', registration %></td>
          <td><%= link_to 'Edit', edit_registration_information_path(registration) %></td>
          <td><%= link_to 'Destroy', registration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
    <% end %>
    </tbody>
  </table>

  <hr>

  <%= link_to 'Register Course', new_registration_information_path %>

</fieldset>